@using MvcCV.EntiyLayer.Concrete
@model List<Experience>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Deneyim Listes</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>SubTitle</th>
        <th>Date</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.SubTtitle</td>
            <td>@item.Date</td>
            <td>
                <a href="/Experience/DeleteExperience/" class="btn btn-danger">Sil</a>
            </td>
            <td> <a href="#" class="btn btn-warning">Düzenle</a></td>
        </tr>

    }
</table>
<a href="/Experience/AddExperience/" class="btn btn-primary">Yeni Deneyim Ekle</a>
