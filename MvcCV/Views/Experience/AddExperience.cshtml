@model Experience
@{
    ViewData["Title"] = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />
<form method="post">


    @Html.Label("Başlık")
    @Html.TextBoxFor(x=>x.Title,new{@class="form-control"})
    <br />
    @Html.Label("Alt Başlık")
    @Html.TextBoxFor(x=>x.SubTtitle,new{@class="form-control"})
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x=>x.Date,new{@class="form-control"})
    <br />


    <button class="btn btn-info">Kaydet</button>

</form>
