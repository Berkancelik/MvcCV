@model Skill
@{
    ViewData["Title"] = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />
<form method="post">
  
    @Html.Label("Yetenek")
    @Html.TextBoxFor(x=>x.Skills,new{@class="form-control"})
    <br />
    @Html.Label("Oran")
    @Html.TextBoxFor(x=>x.Rate,new{@class="form-control"})
    <br />


    <button class="btn btn-info">Kaydet</button>

</form>
